{
  "name": "flashloan",
  "version": "1.0.0",
  "description": "profitable flashloan",
  "license": "ISC",
  "scripts": {
    "generate-types": "typechain --target=ethers-v5 'build/contracts/*.json'",
    "prepare": "tsc",
    "start": "ts-node src/run-arbitrage.ts 2>>error.log",
    "start:windows": "ts-node src/run-arbitrage.ts",
    "fork": "ganache-cli --fork https://mainnet.infura.io/v3/<<infura project id>> --account='<<private key>>,1000000000000000000000'",
    "deploy:fork": "truffle migrate --reset --network mainnetFork",
    "deploy:testnet": "truffle migrate --reset --network testnet",
    "deploy:mainnet": "truffle migrate --reset --network mainnet",
    "deploy:testnet:dry-run": "truffle migrate --reset --network testnet --dry-run",
    "deploy:mainnet:dry-run": "truffle migrate --reset --network mainnet --dry-run",
    "test": "jest --detectOpenHandles",
    "docker:build": "docker build -t flashloan .",
    "docker:run": "docker run -d flashloan"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@studydefi/money-legos": "^2.3.5",
    "@truffle/hdwallet-provider": "^1.0.35",
    "@uniswap/sdk": "^3.0.3-beta.1",
    "chalk": "^4.1.0",
    "csv-writer": "^1.6.0",
    "detect-node": "^2.0.4",
    "dotenv": "^8.2.0",
    "eth-gas-reporter": "^0.2.17",
    "ethers": "^5.0.10",
    "ganache-core": "^2.11.2",
    "mongodb": "^3.5.9",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@extra-array/linspace": "^2.3.18",
    "@typechain/ethers-v5": "^1.0.0",
    "@types/detect-node": "^2.0.0",
    "@types/jest": "^26.0.9",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "babel-jest": "^26.2.2",
    "concurrently": "^5.2.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.54",
    "ts-node": "^8.10.2",
    "typechain": "^2.0.0",
    "typescript": "^3.9.6"
  }
}