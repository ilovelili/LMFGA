{
  "name": "flashloan",
  "author": {
    "name": "min",
    "email": "route666@live.cn",
    "url": "https://www.linkedin.com/in/min-ju"
  },
  "version": "1.0.0",
  "description": "profitable flashloan",
  "license": "ISC",
  "scripts": {
    "postinstall": "patch-package",
    "generate-types": "typechain --target=ethers-v5 'build/contracts/*.json'",
    "prepare": "tsc -p tsconfig.json",
    "start": "ts-node src/run-arbitrage.ts",
    "fetch-price": "ts-node src/fetch-price.ts",
    "fork": "dotenv cross-var ganache-cli --fork %INFURA_URI% --account='%PRIVATE_KEY%,1000000000000000000000'",
    "deploy:local": "truffle migrate --reset --network development",
    "deploy:fork": "truffle migrate --reset --network mainnetFork",
    "deploy:testnet": "truffle migrate --reset --network testnet",
    "deploy:mainnet": "truffle migrate --reset --network mainnet",
    "deploy:testnet:dry-run": "truffle migrate --reset --network testnet --dry-run",
    "deploy:mainnet:dry-run": "truffle migrate --reset --network mainnet --dry-run",
    "test": "jest --detectOpenHandles",
    "docker:build": "docker build -t flashloan .",
    "docker:build:no-cache": "docker build --no-cache -t flashloan .",
    "docker:run": "docker run -d flashloan",
    "fmt": "prettier --write contracts/**/*.sol"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@studydefi/money-legos": "^2.4.1",
    "@truffle/hdwallet-provider": "^1.0.35",
    "@uniswap/sdk": "^3.0.3",
    "chalk": "^4.1.0",
    "console.table": "^0.10.0",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.13",
    "ganache-core": "^2.11.2",
    "moment": "^2.28.0",
    "moment-timezone": "^0.5.31",
    "mongodb": "^3.5.9",
    "nodemailer": "^6.4.14",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@extra-array/linspace": "^2.3.18",
    "@typechain/ethers-v5": "^1.0.0",
    "@types/jest": "^26.0.9",
    "@types/mongodb": "^3.5.25",
    "@types/node": "^14.0.27",
    "@types/nodemailer": "^6.4.0",
    "babel-jest": "^26.2.2",
    "cross-var": "^1.1.0",
    "ganache-cli": "^6.12.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "ts-node": "^8.10.2",
    "typechain": "^2.0.0",
    "typescript": "^3.9.6"
  }
}